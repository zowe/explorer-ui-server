{
  "name": "explorer-ui-server",
  "version": "0.2.20",
  "description": "HTTP Server to host static files",
  "author": "IBM",
  "license": "EPL-2.0",
  "main": "src/index.js",
  "bin": {
    "explorer-ui-server": "src/index.js"
  },
  "publishConfig": {
    "registry": "https://zowe.jfrog.io/zowe/api/npm/npm-local-release/"
  },
  "scripts": {
    "start": "node src/index.js",
    "build": "cp -r configs dist/configs && cp -r src dist/src && cp -r public dist/public && cp *.json dist && cp README.md dist && cp LICENSE dist",
    "prebuild": "npm run clean",
    "clean": "rimraf dist && mkdirp dist",
    "lint": "eslint .",
    "test": "nyc mocha"
  },
  "dependencies": {
    "stdio": "0.2.7"
  },
  "optionalDependencies": {
    "keyring_js": "1.0.0"
  },
  "devDependencies": {
    "abort-controller": "3.0.0",
    "axios": "0.24.0",
    "chai": "4.3.4",
    "debug": "4.3.2",
    "eslint": "7.16.0",
    "mkdirp": "1.0.4",
    "mocha": "9.2.2",
    "mocha-jenkins-reporter": "0.4.7",
    "mocha-multi-reporters": "1.5.1",
    "mochawesome": "7.0.1",
    "node-fetch": "2.6.6",
    "nyc": "15.1.0",
    "rimraf": "3.0.2"
  }
}
